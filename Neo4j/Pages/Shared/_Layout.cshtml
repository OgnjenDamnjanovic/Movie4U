<body>
<!-- header -->
	<div class="header">
		<div class="container">
			<div class="w3layouts_logo">
				<a asp-page="/Index"><img src="images/logo.png"></a>
			</div>
			
			<div class="w3l_sign_in_register">
				<ul>
					<li><i class="fa fa-phone" aria-hidden="true"></i> (+000) 123 345 653</li>
					@if(Model.Message != null)
					{
					<li><a asp-page="/Index" asp-page-handler="Logout" style="width:104px;">Logout</a></li>
					}
					else
					{
					<li><a asp-page="/Login" >Login</a></li>
					}
				</ul>
			</div>
			<div class="clearfix"> </div>
		</div>
	</div>
<!-- //header -->

<!-- nav -->
	<div class="movies_nav">
		<div class="container">
			<nav class="navbar navbar-default">
				<div class="navbar-header navbar-left">
					<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
				</div>
				<!-- Collect the nav links, forms, and other content for toggling -->
				<div class="collapse navbar-collapse navbar-right" id="bs-example-navbar-collapse-1">
					<nav>
						<ul class="nav navbar-nav">
							<li class="active"><a asp-page="/Index">Home</a></li>
							<li><a asp-page="/ExploreMovies">Movies</a></li>
							<li><a asp-page="/ExploreTvShows">tv - series</a></li>
							<li><a asp-page="/News">news</a></li>
							<li><a asp-page="/Contact">Contact Us</a></li>
							@if(Model.Message=="Admin")
							{
								<li><a asp-page="/Admin">Admin</a></li>
							}
							@if(Model.Message=="User")
							{
								<li><a asp-page="/Profile">Profile</a></li>
							}
						</ul>
					</nav>
				</div>
			</nav>	
		</div>
	</div>
<!-- //nav -->

@RenderBody()
        

<!-- footer -->
	<div class="footer">
		<div class="container">
