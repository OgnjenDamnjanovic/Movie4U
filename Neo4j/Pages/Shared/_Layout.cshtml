					<li>
						<a asp-page="/ExploreMovies">Movies</a>
					</li>
					<li>
						<a asp-page="/ExploreTvShows">TV Shows</a>
					</li>
					<li>
						<a asp-page="/News">News</a>
					</li>
					<li>
						<a asp-page="/Contact">Contact Us</a>
					</li>
				</ul>
			</div>
			<div class="clearfix"> </div>
		</div>
	</div>
